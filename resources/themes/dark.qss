QWidget {
    background-color: #333333;
    color: #f0f0f0;
}

QPushButton {
    background-color: #444444;
    color: #f0f0f0;
    padding: 8px 16px;
    border: 1px solid #555555;
    border-radius: 4px;
}

QPushButton:hover {
    background-color: #555555;
}

QPushButton:pressed {
    background-color: #666666;
}
